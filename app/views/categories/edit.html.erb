<%= link_to('Back to List', categories_path, class: 'back-link') %>

<div class="model edit">
  <h2>Edit Category</h2>

  <%= form_for(@category) do |f| %>

    <%= render(partial: "form", locals: {f: f}) %>

    <div class="form-buttons">
      <%= f.submit("Edit Category", class: 'action edit') %>
    </div>

  <% end %>
</div>